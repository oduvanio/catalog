{
	"tpl":"*catalog/search.tpl",
	"jsontpl":"*catalog/search.php?val=&m={infra.Crumb.get.m}&p={infra.Crumb.get.p}",
	"seojsontpl":"*catalog/search.php?seo={infrajs.unicks.catalog.crumb}&val=&m={infra.Crumb.get.m}",
	"autoedittpl":{ "fast":"{infra.conf.catalog.dir}"},
	"showanimate":true,
	"unick":"catalog",
	"configtpl":{
		"root":"{crumb}"
	},
	"child":[{
		"tpl":"*catalog/search.tpl",
		"jsontpl":"*catalog/search.php?val={crumb.name}&m={infra.Crumb.get.m}&p={infra.Crumb.get.p}",
		"seojsontpl":"*catalog/search.php?seo={infrajs.unicks.catalog.crumb}&val=&m={infra.Crumb.get.m}",
		"unick":"search",
		"configtpl":{
			"root":"{crumb.parent}"
		}
	},{
		"child":{
			"tpl":"*catalog/position.tpl",
			"jsontpl":"*catalog/position.php?val={crumb.parent.name}&art={crumb.name}",
			"seojsontpl":"*catalog/position.php?seo={infrajs.unicks.catalog.crumb}&val={crumb.parent.name}&art={crumb.name}",
			"seotpl":{"link":"{crumb.parent.parent}/{{producer}}/{{article}}"}
		}
	}],
	"childs":{
		"find":{
			"jsontpl":"*catalog/find.php",
			"seojsontpl":"*catalog/find.php?seo={infrajs.unicks.catalog.crumb}",
			"configtpl":{
				"root":"{crumb.parent}"
			},
			"tpl":"*catalog/find.tpl",
			"autosavename":"catalog"
		},
		"producers":{
			"tpl":"*catalog/producers.tpl",
			"jsontpl":"*catalog/producers.php?lim=0,200&m={infra.Crumb.get.m}",
			"seojsontpl":"*catalog/producers.php?seo={infrajs.unicks.catalog.crumb}",
			"autoedit":{ "fast":"*Тексты/Производители"},
			"configtpl":{
				"root":"{crumb.parent}"
			}
		},
		"stat":{
			"tpl":"*catalog/stat.tpl",
			"json":"*catalog/stat.php",
			"seojsontpl":"*catalog/stat.php?seo={infrajs.unicks.catalog.crumb}",
			"autoedit":{ "fast":"*Тексты/Статистика поиска по каталогу"},
			"configtpl":{
				"root":"{crumb.parent}"
			}
		}
	}
}
